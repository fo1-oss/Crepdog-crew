{
  "name": "cdc-backend",
  "version": "1.0.0",
  "description": "Backend API for CDC Investor Portal with authentication, analytics, and OCR",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=integration",
    "lint": "eslint src"
  },
  "keywords": [
    "investor-portal",
    "analytics",
    "ocr",
    "authentication"
  ],
  "author": "CDC Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/vision": "^4.3.2",
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "googleapis": "^144.0.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pdf-parse": "^1.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0"
  }
}